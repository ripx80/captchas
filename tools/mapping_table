# Primary 3-bit           # Equivalent "bright" versions
 00 = 000000               08 = 808080
 01 = 800000               09 = ff0000
 02 = 008000               10 = 00ff00
 03 = 808000               11 = ffff00
 04 = 000080               12 = 0000ff
 05 = 800080               13 = ff00ff
 06 = 008080               14 = 00ffff
 07 = c0c0c0               15 = ffffff

